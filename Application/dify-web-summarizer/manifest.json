{
  "manifest_version": 3,
  "name": "dify-web-summarizer",
  "version": "1.0",
  "description": "Summarizes web pages.",
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "src/background/background.js"
  }
}
